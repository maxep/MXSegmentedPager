ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/HMSegmentedControl" "$PODS_CONFIGURATION_BUILD_DIR/MXPagerView" "$PODS_CONFIGURATION_BUILD_DIR/MXParallaxHeader" "$PODS_CONFIGURATION_BUILD_DIR/MXSegmentedPager"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/HMSegmentedControl/HMSegmentedControl.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MXPagerView/MXPagerView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MXParallaxHeader/MXParallaxHeader.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MXSegmentedPager/MXSegmentedPager.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "HMSegmentedControl" -framework "MXPagerView" -framework "MXParallaxHeader" -framework "MXSegmentedPager"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
